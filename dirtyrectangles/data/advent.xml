<?xml version="1.0"?>
<resources>
  <sprite name="font_glyphs">
    <image name="image1" file="images/verdana20_cyan.png"><alpha /></image>
  </sprite>

  <font name="font">
    <bitmap glyphs="font_glyphs" letters="!_#$%&'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\]^_`abcdefghijklmnopqrstuvwxyz{|}~" />
