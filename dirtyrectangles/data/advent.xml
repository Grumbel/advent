  <sprite name="background2">
    <image file="images/background2.jpg" />
  </sprite>

  <sprite name="person">
    <image file="images/person.png" />
    <translation origin="bottom_center" />
  </sprite>

  <sprite name="tree">
    <image file="images/tree.png" />
  </sprite>
  <sprite name="button">
    <image file="images/button.png" />
  </sprite>
  <sprite name="button_down">
    <image file="images/button_down.png" />
  </sprite>
